

<form action="personalInformation" method="POST" class="form-horizontal">
    <div class="form-group">
        <label for="exampleInputEmail1" class="col-sm-2 control-label">loginName</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="exampleInputEmail1" placeholder="loginName" name="loginName">
        </div>
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1" class="col-sm-2 control-label">gender</label>
        <div class="col-sm-10">
            <label class="radio-inline">
                <input type="radio" name="male" id="inlineRadio1" value="option1" > 男
            </label>
            <label class="radio-inline">
                <input type="radio" name="female" id="inlineRadio2" value="option2"> 女
            </label>
        </div>
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1" class="col-sm-2 control-label">phone</label>
        <div class="col-sm-10">
            <input type="tel" class="form-control" id="exampleInputPassword1" placeholder="phone" name="phone">
        </div>
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1" class="col-sm-2 control-label">email</label>
        <div class="col-sm-10">
            <input type="email" class="form-control" id="exampleInputPassword1" placeholder="email" name="email">
        </div>
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1" class="col-sm-2 control-label">hashpwd</label>
        <div class="col-sm-10">
            <input type="text" class="form-control"  placeholder="hashpwd" name="hashpwd">
        </div>

    </div>
    <div class="form-group">
        <label for="exampleInputPassword1" class="col-sm-2 control-label">realName</label>
        <div class="col-sm-10">
            <input type="text" class="form-control"  placeholder="realName" name="realName">
        </div>

    </div>
    <div class="form-group">
        <label for="exampleInputPassword1" class="col-sm-2 control-label">nickName</label>
        <div class="col-sm-10">
            <input type="text" class="form-control"  placeholder="nickName" name="nickName">
        </div>
    </div>
    <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10">
            <input type="submit" class="btn btn-default" value="Send"/>
        </div>
    </div>
</form>